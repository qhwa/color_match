{

  parser: 'babel-eslint',

  plugins: [],

  parserOptions: {
    ecmaVersion: 6,
    sourceType: 'module',
    ecmaFeatures: {
      jsx: true
    }
  },

  env: {
    browser: true,
    amd: true,
    es6: true,
    node: true,
    mocha: true
  },


  globals: {
    expect: true
  },


  extends: ['eslint:recommended', 'plugin:react/recommended'],


  rules: {
    'comma-dangle': 2,
    'no-cond-assign': 2,
    'no-console': 1,
    'no-constant-condition': 2,
    'no-control-regex': 2,
    'no-debugger': 2,
    'no-dupe-args': 2,
    'no-dupe-keys': 2,
    'no-duplicate-case': 2,
    'no-empty-character-class': 2,
    'no-empty': 2,
    'no-ex-assign': 2,
    'no-extra-boolean-cast': 2,
    'no-extra-semi': 2,
    'no-func-assign': 2,
    'no-inner-declarations': 2,
    'no-invalid-regexp': 2,
    'no-irregular-whitespace': 2,
    'no-negated-in-lhs': 2,
    'no-obj-calls': 2,
    'no-regex-spaces': 2,
    'no-sparse-arrays': 2,
    'no-unreachable': 2,
    'use-isnan': 2,
    'valid-jsdoc': [1, { requireReturn: false }],
    'valid-typeof': 2,


    'accessor-pairs': 1,
    'block-scoped-var': 1,
    'complexity': [1, 10],
    'consistent-return': 0,
    'curly': 1,
    'default-case': 1,
    'dot-notation': 1,
    'dot-location': [1, 'property'],
    'eqeqeq': 1,
    'guard-for-in': 0,
    'no-alert': 1,
    'no-caller': 1,
    'no-div-regex': 2,
    'no-else-return': 1,
    'no-labels': 2,
    'no-eq-null': 1,
    'no-eval': 2,
    'no-extend-native': 2,
    'no-extra-bind': 2,
    'no-fallthrough': 2,
    'no-floating-decimal': 2,
    'no-implied-eval': 2,
    'no-iterator': 2,
    'no-lone-blocks': 2,
    'no-loop-func': 1,
    'no-multi-str': 1,
    'no-native-reassign': 2,
    'no-new-func': 2,
    'no-new-wrappers': 2,
    'no-new': 1,

    'no-octal-escape': 2,

    'no-octal': 2,


    'no-process-env': 2,

    'no-proto': 2,

    'no-redeclare': 2,

    'no-return-assign': 2,

    'no-script-url': 2,

    'no-self-compare': 2,

    'no-sequences': 2,

    'no-throw-literal': 2,

    'no-unused-expressions': 0,

    'no-void': 2,

    'no-warning-comments': 1,

    'no-with': 2,

    'radix': 2,

    'vars-on-to': 0,

    'wrap-iife': [2, 'inside'],

    'yoda': 1,

    'strict': 0,



    'no-delete-var': 2,

    'no-label-var': 2,

    'no-shadow-restricted-names': 2,

    'no-shadow': 2,

    'no-undef-init': 2,

    'no-undef': 2,

    'no-undefined': 0,

    'no-unused-vars': 2,

    'no-use-before-define': 0,



    'handle-callback-err': 2,

    'no-mixed-requires': 2,

    'no-new-require': 2,

    'no-path-concat': 2,

    'no-process-exit': 2,





    'brace-style': [1, '1tbs', { allowSingleLine: true }],

    'camelcase': [1, { properties: 'always' }],

    'comma-spacing': [1, { before: false, after: true }],

    'comma-style': [1, 'last'],

    'computed-property-spacing': [1, 'never'],


    'eol-last': 1,



    'indent': [1, 2, { SwitchCase: 1 }],


    'lines-around-comment': 1,

    'linebreak-style': [1, 'unix'],

    'max-nested-callbacks': [1, 4],

    'new-cap': 1,

    'new-parens': 1,


    'no-array-constructor': 1,

    'no-continue': 1,


    'no-lonely-if': 1,

    'no-mixed-spaces-and-tabs': [1, 'smart-tabs'],

    'no-multiple-empty-lines': [1, { max: 3 }],


    'no-new-object': 1,

    'no-spaced-func': 1,


    'no-trailing-spaces': 1,


    'no-unneeded-ternary': 1,






    'quote-props': [1, 'as-needed'],

    'quotes': [1, 'single', 'avoid-escape'],

    'semi-spacing': [1, { before: false, after: true }],

    'semi': 1,



    'keyword-spacing': [1],

    'space-before-blocks': [1, 'always'],

    'space-before-function-paren': [1, 'never'],

    'space-in-parens': [1, 'never'],

    'space-infix-ops': 1,

    'space-unary-ops': [1, { words: true, nonwords: false }],


    'wrap-regex': 1,

    'max-depth': [1, 4],

    'max-len': [1, 90, 2],

    'max-params': [1, 4],

    'max-statements': [1, 20],

    'no-bitwise': 1,

    'no-case-declarations': 1,



    'no-var': 1,

    'prefer-const': 1
  }
}
